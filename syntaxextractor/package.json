{
  "name": "syntaxextractor",
  "displayName": "SyntaxExtractor",
  "description": "A Program that extracts Function names and Class names from a Project",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "syntaxextractor.button1",
        "title": "Button 1"
      },
      {
        "command": "syntaxextractor.button2",
        "title": "Button 2"
      },
      {
        "command": "syntaxextractor.button3",
        "title": "Button 3"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "syntaxExtractor",
          "title": "SyntaxExtractor",
          "icon": "resources/syntaxExtractor-icon.svg"
        }
      ]
    },
    "views": {
      "syntaxExtractor": [
        {
          "id": "syntaxExtractorCustomView",
          "name": "SyntaxExtractor Custom View"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vscode-languageserver": "^8.1.0",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "omnisharp-client": "^3.2.0",
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver-protocol": "^3.17.3",
    "vscode-languageserver-textdocument": "^1.0.8",
    "web-tree-sitter": "^0.20.8"
  },
  "license": "MIT"
}
